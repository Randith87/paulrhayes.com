<% post = page %>
<main class="article article-note h-entry">
  <div class="article-meta" role="contentinfo">
    <%- partial('_partial/post/nav') %>
  </div>
  <header class="article-header">
    <h1 class="main-content-title entry-title p-name">
      <span class="main-content-subtitle"><a href="/notes/">Notes</a><% if (page.thing) { %> on <%= page.thing %><% } %></span>
      <%= page.title %>
    </h1>
  </header>
  <div class="article-content entry-content e-content">
    <%- page.content %>
    <p>
      <%- partial('_partial/note/published') %>
    </p>
  </div>
</main>
